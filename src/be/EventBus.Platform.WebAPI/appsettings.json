{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EventBus": {
    "Database": {
      "ConnectionString": "InMemoryDatabase",
      "Provider": "InMemory"
    },
    "Cache": {
      "DefaultExpiration": "00:30:00"
    },
    "Queue": {
      "ProviderType": "Channel",
      "DefaultCapacity": 1000,
      "EnableLogging": true,
      "FullMode": "Wait"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
